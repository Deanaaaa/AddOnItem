<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="155dp"
    android:background="#3b6967">
    <LinearLayout
        android:id="@+id/q_id_layout_mine"
        android:layout_marginLeft="10dp"
        android:layout_marginTop="17.5dp"
        android:layout_width="105dp"
        android:layout_height="22dp">
    <ImageView
        android:id="@+id/q_id_img_tab5_back_icon"
        android:layout_width="18dp"
        android:layout_height="22dp"
        android:src="@drawable/back_icon"/>
    <TextView
        android:id="@+id/q_id_text_tab5_mine"
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="个人主页"
        android:textSize="17sp"
        android:textColor="#ffffff"/>
    </LinearLayout>
    <ImageView
        android:id="@+id/q_id_img_tab5_mine_head"
        android:layout_marginTop="54.5dp"
        android:layout_centerHorizontal="true"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:src="@drawable/mine_head_icon"/>
    <TextView
        android:id="@+id/q_id_text_tab5_name"
        android:layout_marginTop="122dp"
        android:layout_centerHorizontal="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="用户名"
        android:textSize="17sp"
        android:textColor="#ffffff"/>


</RelativeLayout>