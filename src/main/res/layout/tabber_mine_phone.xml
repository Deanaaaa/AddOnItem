<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="48dp"
    android:background="#ffffff"
    android:id="@+id/q_id_layout_change">
    <ImageView
        android:id="@+id/q_id_img_tab5_mine_phone"
        android:layout_width="18dp"
        android:layout_height="29dp"
        android:layout_marginLeft="77dp"
        android:layout_marginTop="9dp"
        android:src="@drawable/mine_phone_icon"/>
    <TextView
        android:id="@+id/q_id_text_tab5_mine_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="63dp"
        android:layout_marginTop="15dp"
        android:text="切换账号"
        android:textSize="17sp"
        android:textColor="#333333"/>
    <ImageView
        android:id="@+id/q_id_img_tab5_mine_phone_more"
        android:layout_marginLeft="63dp"
        android:layout_marginTop="15dp"
        android:layout_width="17dp"
        android:layout_height="20.33dp"
        android:src="@drawable/mine_more_icon"/>

</LinearLayout>